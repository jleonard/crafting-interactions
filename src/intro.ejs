<%
  function rando(len){
    var str ='';
    var a = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ'.split('');
    for(var ii = 0; ii < len; ii++){
      str += a[Math.floor(Math.random()*a.length)];
    }
    return str;
  }
%>

